{
  "name": "unfun",
  "license": "MIT",
  "version": "0.0.0",
  "packageManager": "pnpm@9.8.0",
  "homepage": "https://unfun.vercel.app",
  "description": "Type-safe functional programming library written in TypeScript",
  "repository": "https://github.com/wagerfield/unfun.git",
  "bugs": "https://github.com/wagerfield/unfun/issues",
  "author": {
    "name": "Matthew Wagerfield",
    "email": "matthew@wagerfield.com",
    "url": "https://matthew.wagerfield.com"
  },
  "keywords": [
    "functional",
    "programming",
    "point-free",
    "type-safe",
    "curried",
    "tacit",
    "pure"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/unfun.cjs",
  "module": "./dist/unfun.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/unfun.js",
      "require": "./dist/unfun.cjs"
    }
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "clean": "rm -rf .astro .vercel astro dist",
    "format": "prettier --write .",
    "test:watch": "vitest watch",
    "test:once": "vitest run",
    "package": "vite build",
    "release": "release-it",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "vitest related --run"
    ],
    "**/*.{json,md,mdx}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "semi": false
  },
  "devDependencies": {
    "@astrojs/check": "0.8.1",
    "@astrojs/tailwind": "5.1.0",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@fontsource-variable/inter": "5.0.19",
    "@fontsource-variable/jetbrains-mono": "5.0.21",
    "@vitest/coverage-v8": "2.0.3",
    "astro": "4.11.5",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "release-it": "17.6.0",
    "tailwindcss": "3.4.5",
    "typescript": "5.5.3",
    "vite": "5.3.4",
    "vite-plugin-dts": "3.9.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.3"
  }
}
